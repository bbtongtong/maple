package chapter1.sec01;

public class Employee {
	String empno;
	String empname;
	String deptname;
	String rank;
	int age;
	String gender;
	double anuSalary;
	String telno;
	String address;
	
	public double getWage() {
		return anuSalary;
	}
	
	public void setWage(double anuSalary) {
		anuSalary=anuSalary/12;
		System.out.println( "급여는 "+anuSalary+"원 입니다.");
		anuSalary=anuSalary*0.92;
		System.out.println( "세금을 제외 받게될 금액은"+anuSalary+"원 입니다.");
		this.anuSalary=anuSalary;
		
	}

}
